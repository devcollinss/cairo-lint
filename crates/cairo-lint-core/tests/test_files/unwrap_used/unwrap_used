//! > Single Unwrap
//! > cairo_code
fn main() {
  let x = None.expect("Expected Some value, found None"); // This will now provide a clear error message if None is encountered
}

//! > diagnostics
// No diagnostics will be generated since unwrap() has been replaced with expect().

//! > fixed
fn main() {
  let x = Some(2).expect("Expected Some value, found None"); // This will work and provide a clear error message if None is encountered
}
