//! > Single Unwrap
//! > cairo_code
fn main() {
  let _x = Option::<i32>::None.expect("Expected Some value, found None"); // Explicitly specify Option type
}

//! > diagnostics
// No diagnostics will be generated since unwrap() has been replaced with expect().

//! > fixed
fn main() {
  let _x = Some(2).expect("Expected Some value, found None"); // This will work and provide a clear error message if None is encountered
}
